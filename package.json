    "generate": "yarn workspace @scaffold-eth/hardhat generate",
    "account": "yarn workspace @scaffold-eth/hardhat account",
    "flatten": "cd packages/hardhat && npx hardhat flatten",
    "clean": "cd packages/hardhat && npx hardhat clean",
